									<div class="ticket">
										<div class="ticketUpperArea">
											<div class="movieName"><%= movieBriefInfo.name %></div>
											<div class="category"><%= ticket.ticketCategory %></div>
										</div>

										<div class="cinema">
											<%= movieBriefInfo.cinema %>
										</div>

										<div class="ticketBottomArea">
											<div class="date">
												<span>Date</span>
												<span><%= ticket.date %></span>
											</div>
		
											<div class="time">
												<span>Time</span>
												<span><%= ticket.time %></span>
											</div>
		
											<div class="seat">
												<span>Seat</span>
												<span><%= ticket.seat %></span>
											</div>
											
											<div class="price">
												<span>Price</span>
