	<body>
		<div id="fh5co-wrapper">
		<div id="fh5co-page">
		<% let activeNav = "Order" %>
		<% include ../menuHeader %>
		<!-- end:fh5co-header -->
		
		<div id="fh5co-contact" class="fh5co-lightgray-section topStageBar" >
			<div class="container">
				<div class="row"> 
						<div class="row bs-wizard" style="border-bottom:0;">
							<div class="col-xs-2 col-md-offset-1 bs-wizard-step complete">
								<div class="text-center bs-wizard-stepnum">Step 1</div>
								<div class="progress"><div class="progress-bar"></div></div>
								<a href="#" class="bs-wizard-dot"></a>
								<div class="bs-wizard-info text-center">Buy Tickets</div>
							</div>
							
							<div class="col-xs-2 bs-wizard-step complete"><!-- complete -->
								<div class="text-center bs-wizard-stepnum">Step 2</div>
								<div class="progress"><div class="progress-bar"></div></div>
								<a href="#" class="bs-wizard-dot"></a>
								<div class="bs-wizard-info text-center">Select Seats</div>
							</div>
							
							<div class="col-xs-2 bs-wizard-step complete"><!-- complete -->
								<div class="text-center bs-wizard-stepnum">Step 3</div>
								<div class="progress"><div class="progress-bar"></div></div>
								<a href="#" class="bs-wizard-dot"></a>
								<div class="bs-wizard-info text-center">Confirm Order</div>
							</div>
							
							<div class="col-xs-2 bs-wizard-step complete"><!-- active -->
								<div class="text-center bs-wizard-stepnum">Step 4</div>
								<div class="progress"><div class="progress-bar"></div></div>
								<a href="#" class="bs-wizard-dot"></a>
								<div class="bs-wizard-info text-center">Pay for Order</div>
							</div>

							<div class="col-xs-2 bs-wizard-step complete"><!-- active -->
								<div class="text-center bs-wizard-stepnum">Step 5</div>
								<div class="progress"><div class="progress-bar"></div></div>
								<a href="#" class="bs-wizard-dot"></a>
								<div class="bs-wizard-info text-center">Success</div>
							</div>
						</div>
				</div>
			</div>
		</div>

		<div id="fh5co-contact">
			<div class="container">
				<div class="row">
					<div class="col-md-8 col-md-offset-2">
						<div class="heading-section text-center animate-box">
							<h2>Success!</h2>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col-md-2 col-md-offset-4 animate-box" id="statusIcon">
						<i class="fas fa-check-circle"></i>
					</div>
					<div class="col-md-6 animate-box" id="orderMessage">
						<span>Your Order No.</span>
						<span><%= orderId %></span>
					</div>
				</div>
				<div class="row" id="successTickets">
					<div id="tickets" class="col-md-6 col-md-offset-3">
						<% for (ticket of tickets) { %>
							<div class="ticket">
								<div class="ticketUpperArea">
									<div class="movieName"><%= movieBriefInfo.name %></div>
									<div class="category"><%= ticket.ticketCategory %></div>
								</div>

								<div class="cinema">
									<%= movieBriefInfo.cinema %>
								</div>

								<div class="ticketBottomArea">
									<div class="date">
										<span>Date</span>
										<span><%= ticket.date %></span>
									</div>

									<div class="time">
										<span>Time</span>
										<span><%= ticket.time %></span>
									</div>

									<div class="seat">
										<span>Seat</span>
										<span><%= ticket.seat %></span>
									</div>
									
									<div class="price">
										<span>Price</span>
										<span>$<%= ticket.price %></span>
									</div>
								</div>
								
							</div>
						<% } %>
					</div>
				</div>

				<div class="row" id="successTickets">
					<div class="col-xs-2 col-md-offset-5">
						<button class="subscribe btn btn-success btn-lg btn-block nextButton" type="button">Done</button>
					</div>
				</div>
				
			</div>
		</div>
		<!-- END fh5co-contact -->

		<% include ../footer %>
	

	</div>
	<!-- END fh5co-page -->

	</div>
	<!-- END fh5co-wrapper -->

	<!-- jQuery -->
	<script src="../../js/jquery.min.js"></script>
