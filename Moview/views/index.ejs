	<body>
		<div id="fh5co-wrapper">
		<div id="fh5co-page">
		<% include menuHeader %>
		<!-- end:fh5co-header -->

		<div id="moviePosterCarousel">
			<div class="row">
				<div class="col-md-12">
					<div id="myCarousel" class="carousel slide" data-ride="carousel">
						<!-- Indicators -->
						<ol class="carousel-indicators">
						<li data-target="#myCarousel" data-slide-to="0" class="active"></li>
						<li data-target="#myCarousel" data-slide-to="1"></li>
						<li data-target="#myCarousel" data-slide-to="2"></li>
						</ol>
					
						<!-- Wrapper for slides -->
						<div class="carousel-inner">
						<div class="item active">
							<img src="images/deadpool2.jpg">
						</div>
					
						<div class="item">
							<img src="images/quietPlace.jpg">
						</div>
						
						<div class="item">
							<img src="images/avengers.jpg">
						</div>
						</div>
					
						<!-- Left and right controls -->
						<a class="left carousel-control" href="#myCarousel" data-slide="prev">
						<span class="glyphicon glyphicon-chevron-left"></span>
						<span class="sr-only">Previous</span>
						</a>
						<a class="right carousel-control" href="#myCarousel" data-slide="next">
						<span class="glyphicon glyphicon-chevron-right"></span>
						<span class="sr-only">Next</span>
						</a>
					</div>
				</div>
			</div>
		</div>

		<div id="fh5co-team-section" class="fh5co-lightgray-section">
			<div class="container">
				<div class="row">
					<div class="col-md-8 col-md-offset-2">
						<div class="heading-section text-center animate-box">
							<h2>In Theaters</h2>
							<p> A movie a day keep happiness today.</p>
						</div>
					</div>
				</div>
				<div class="row text-center">
					<% for (inTheaterMovie of inTheaterMovies) { %>
						<div class="col-md-3 col-sm-6">
							<div class="team-section-grid animate-box" onclick="redirect('./movieDetail/<%= inTheaterMovie._id %>')" style="background-image: url(<%= inTheaterMovie.coverUrl %>);">
								<div class="overlay-section">
									<div class="desc">
										<h3><%= inTheaterMovie.name %></h3>
										<span><%= inTheaterMovie.runtime %> min</span>
										<p><%= inTheaterMovie.releaseDate %></p>
										<p class="fh5co-social-icons">
											<a href="#"><i class="icon-twitter-with-circle"></i></a>
											<a href="#"><i class="icon-facebook-with-circle"></i></a>
											<a href="#"><i class="icon-instagram-with-circle"></i></a>
										</p>
									</div>
								</div>
							</div>
						</div>
					<% } %>
				</div>
			</div>
		
		</div>
		
		<div id="fh5co-team-section" >
			<div class="container">
				<div class="row">
					<div class="col-md-8 col-md-offset-2">
						<div class="heading-section text-center animate-box">
							<h2>Coming Soon...</h2>
							<p> Good things are worth being waited for.</p>
						</div>
					</div>
				</div>
				<div class="row text-center">
					<% for (comingSoonMovie of comingSoonMovies) { %>
						<div class="col-md-3 col-sm-6">
							<div class="team-section-grid animate-box" onclick="redirect('./movieDetail/<%= comingSoonMovie._id %>')" style="background-image: url(<%= comingSoonMovie.coverUrl %>);">
								<div class="overlay-section">
									<div class="desc">
										<h3><%= comingSoonMovie.name %></h3>
										<span><%= comingSoonMovie.runtime %> min</span>
										<p><%= comingSoonMovie.releaseDate %></p>
										<p class="fh5co-social-icons">
											<a href="#"><i class="icon-twitter-with-circle"></i></a>
											<a href="#"><i class="icon-facebook-with-circle"></i></a>
											<a href="#"><i class="icon-instagram-with-circle"></i></a>
										</p>
									</div>
								</div>
							</div>
						</div>
					<% } %>
					
				</div>
			</div>
		</div>

		<div class="fh5co-parallax" style="background-image: url(images/cinemaPopcorn.jpg);" data-stellar-background-ratio="0.5">
			<div class="overlay"></div>
			<div class="container">
				<div class="row">
					<div class="col-md-6 col-md-offset-3 col-md-pull-3 col-sm-12 col-sm-offset-0 col-xs-12 col-xs-offset-0 fh5co-table">
						<div class="fh5co-intro fh5co-table-cell box-area">
							<div class="animate-box">
								<h1>Awsome Movies Platform</h1>
								<p>Write it, Share it, Love it.</p>
								<a href="#" class="btn btn-primary">Become A Member</a>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- end: fh5co-parallax -->
		
		<% include footer %>
	

	</div>
	<!-- END fh5co-page -->

	</div>
