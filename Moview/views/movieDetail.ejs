	<script>
		$('.imgsCarouselDiv').owlCarousel({
			margin:10,
			loop:true,
			items: 4,
			autoplay: true
		})

		$('.videosCarouselDiv').owlCarousel({
			margin:10,
			items:1,
			loop:true,
			lazyLoad:true,
			center:true
		})


		let checkIsLogin = () => {
			if (!<%= user.isLogin %>) {
				document.location.href = "/auth/facebook";
				return false;
			}
			return true;
		}

		// Reviews
		let reviews = <%- JSON.stringify(reviews) %>;
		
		if (reviews.length != 0) {
			$('#pagination').twbsPagination({
				totalPages: Math.ceil(reviews.length / 5),
				visiblePages: 5,
				onPageClick: function (event, page) {
					let reviewsHTMLString = getReviewsHTMLString(reviews.slice((page-1)*5, page*5));
					$('#reviewList').html(reviewsHTMLString);
				}
			});
		}
		else {
			$('#reviewList').html("<h4>No Review...<h4>");
		}

		function getReviewsHTMLString(reviews) {
			let reviewsHTMLString = "";
			for (review of reviews) {
				let likeReviewActiveStr = "";
				let dislikeReviewActiveStr = "";
				if (<%= user.isLogin %> && review.likeReviewMemberIds.includes("<%= user._id %>"))
					likeReviewActiveStr = "active";
				if (<%= user.isLogin %> && review.dislikeReviewMemberIds.includes("<%= user._id %>"))
					dislikeReviewActiveStr = "active";

				reviewsHTMLString += ` \
						<div class=\"row\"> \
							<div class=\"col-md-12\"> \
								<div class=\"reviewTopDiv\"> \
									<div> \
										<h3>${review.title}</h3> \
									</div> \
									<div class=\"likeDiv\"> \
										<i data-reviewid="${review._id}" class=\"fas fa-thumbs-up likeReviewButton ${likeReviewActiveStr}\"></i> <span class=\"likeAmount\">${review.likeAmount}</span> \
										<i data-reviewid="${review._id}" class=\"fas fa-thumbs-down dislikeReviewButton ${dislikeReviewActiveStr}\"></i> <span class=\"dislikeAmount\">${review.dislikeAmount}</span> \
									</div> \
								</div> \
								<div class=\"reviewMidDiv\"> \
									<span><a>${review.memberName}</a></span> \
									<span>${review.createTime}</span> \
								</div> \
								<div class=\"reviewContent\"> \
									<p> ${review.content} </p> \
								</div> \
							</div> \
						</div> \
						<hr>` ;
			}

			return reviewsHTMLString;
		}

		$('#writeReviewButton').click(function() {
			checkIsLogin();
		})

		$("#submitReviewButton").click(function() {
			$.ajax({
				method: "POST",
				contentType:"application/json; charset=utf-8",
				url: "/review/enterReview",
				data: JSON.stringify(
					{
						movieId: "<%= movie._id %>", 
						title: $('#reviewTitle').val(), 
						content: $('#reviewContent').val(),
					}
				)
			}).done(function (msg) {
				document.location.reload();
			});
		});

		let likeReviewHttpRequest = (url, reviewId) => {
			$.ajax({
				method: "POST",
				contentType:"application/json; charset=utf-8",
				url: url,
				data: JSON.stringify(
					{
						reviewId: reviewId,
					}
				)
			}).done(function (msg) {
				if (msg === "User hasn't login")
					document.location.href = "/auth/facebook";
			});
		}
		
		$(document).on("click", ".likeReviewButton", function() {
			if (!checkIsLogin())
				return;
			let likeAmount = parseInt($(this).next().text());
			if (!$(this).hasClass("active")) {
				likeReviewHttpRequest("/review/likeReview", $(this).data("reviewid"));
				$(this).addClass("active");
				$(this).next().text(likeAmount + 1);
			}
			else {
				likeReviewHttpRequest("/review/cancelLikeReview", $(this).data("reviewid"));
				$(this).removeClass("active");
				$(this).next().text(likeAmount - 1);
			}
		});

		$(document).on("click", ".dislikeReviewButton", function() {
			if (!checkIsLogin())
				return;
			let dislikeAmount = parseInt($(this).next().text());
			if (!$(this).hasClass("active")) {
				likeReviewHttpRequest("/review/dislikeReview", $(this).data("reviewid"));
				$(this).addClass("active");
				$(this).next().text(dislikeAmount + 1);
			}
			else {
				likeReviewHttpRequest("/review/cancelDislikeReview", $(this).data("reviewid"));
				$(this).removeClass("active");
				$(this).next().text(dislikeAmount - 1);
			}
		});

	</script>


	</body>
</html>

