		<div id="fh5co-contact">
			<div class="container">
				<form action="#">
					<div class="row movieInfo">
						<h1 class="col-md-12 movie-title animate-box"><%= movie.name %></h1>
						<div class="col-md-4 animate-box movieImgDiv">
							<img src="<%= movie.coverUrl %>">
						</div>
						<div class="col-md-8 animate-box">
							<div class="reviewRatingArea">
								<i class="fa fa-star" style="font-size:30px;color:gold"></i> 4.8
							</div>
							
							<div class="infoArea">
								<div class="infoAreaTitle">Movie Info</div>
								<hr>
								<table>
									<tr>
										<td>Directors:</td>
										<td>
											<% for (var i=0; i < movie.directors.length; i++) { %>
												<a><%= movie.directors[i] %></a>
												<%= (i == movie.directors.length-1) ?  '' : ',' %>
											<% } %>
										</td>
									</tr>
									<tr>
										<td>Writers:</td>
										<td><a>Rhett Reese, Wernick</a></td>
									</tr>
									<tr>
										<td>Stars:</td>
										<td>
											<% for (var i=0; i < movie.casts.length; i++) { %>
												<a><%= movie.casts[i] %></a>
												<%= (i == movie.casts.length-1) ?  '' : ',' %>
											<% } %>
										</td>
									</tr>

									<% if (typeof movie.runtime != "undefined") { %>
										<tr>
											<td>Runtime:</td>
											<td><%= movie.runtime %> min</td>
										</tr>
									<% } %>
									
									<% if (typeof movie.releaseDate != "undefined") { %>
										<tr>
											<td>In Theaters:</td>
											<td><%= movie.releaseDate %></td>
										</tr>
									<% } %>

									<tr>
										<td>Category:</td>
										<td>
											<% for (var i=0; i < movie.categories.length; i++) { %>
												<a><%= movie.categories[i] %></a>
												<%= (i == movie.categories.length-1) ?  '' : ',' %>
											<% } %>
										</td>
									</tr>

								</table>
							</div>

							<div class="storylineArea">
								<div class="infoAreaTitle">Storyline</div>
								<hr>
								<p>
									<%= movie.storyline %>
								</p>
								<span class="seeDetailBtn"><a>See full storyline >></a></span>
							</div>
						</div>
					</div>
				</form>
